{
  "name": "willwashburn/canonical",
  "description": "return the canonical url from a string of html",
  "type": "library",
  "authors": [
    {
      "name": "Will Washburn",
      "email": "will.washburn@gmail.com"
    }
  ],
  "keywords": [
    "canonical",
    "canonical url",
    "og url"
  ],
  "homepage": "https://github.com/willwashburn/canonical",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "symfony/css-selector": "2.*",
    "symfony/dom-crawler": "2.*"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "3.*"
  },
  "scripts": {
    "fix": "vendor/bin/phpcbf src/ tests/Test.php --standard=PSR2",
    "test": "vendor/bin/phpcs src/ --standard=PSR2 && vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "WillWashburn\\": "src/"
    }
  },
  "minimum-stability": "stable"
}